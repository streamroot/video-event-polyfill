{
  "name": "video-event-polyfill",
  "version": "0.1.0",
  "description": "polyfill for unconsistent events of HTML5 video API",
  "homepage": "https://github.com/streamroot/video-event-polyfill",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:streamroot/video-event-polyfill.git"
  },
  "scripts": {
    "build": "mkdir -p dist && browserify lib/video-event-polyfill.js | uglifyjs > dist/video-event-polyfill.js"
  },
  "author": "Axel Delmas",
  "keywords": [
    "html5",
    "video",
    "events",
    "polyfill"
  ],
  "license": "ISC",
  "main": "dist/video-event-polyfill",
  "browserify": {
    "transform": [
      "babelify"
    ],
    "plugins": [
      "browserify-derequire"
    ]
  },
  "dependencies": {
    "babelify": "^7.3.0",
    "browserify": "^13.0.1"
  },
  "devDependencies": {
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-es2015": "^6.9.0",
    "browserify-derequire": "^0.9.4",
    "uglify-js": "^2.6.2"
  }
}
